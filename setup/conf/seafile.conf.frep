[fileserver]
port = 8082
{{ if ge ( .Env.SEAFILE_VERSION_MAJOR | default "8" | int ) 9 }}
use_go_fileserver = true
{{ end }}

[database]
type = mysql
host = {{.Env.MYSQL_HOST}}
port = {{.Env.MYSQL_PORT}}
user = {{.Env.SEAFILE_USER}}
db_name = {{.Env.SEAFILE_DB}}
password = {{.Env.MYSQL_PASSWORD}}
connection_charset = utf8

